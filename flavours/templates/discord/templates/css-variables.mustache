	--accentcolor:					{{base0F-rgb-r}},{{base0F-rgb-g}},{{base0F-rgb-b}};
	--accentcolor2:					{{base0E-rgb-r}},{{base0E-rgb-g}},{{base0E-rgb-b}};
	--linkcolor:					  {{base0C-rgb-r}},{{base0C-rgb-g}},{{base0C-rgb-b}};

	--mentioncolor: 				{{base08-rgb-r}},{{base08-rgb-g}},{{base08-rgb-b}};
	--successcolor: 				{{base0B-rgb-r}},{{base0B-rgb-g}},{{base0B-rgb-b}};
	--warningcolor: 				{{base0A-rgb-r}},{{base0A-rgb-g}},{{base0A-rgb-b}};
	--dangercolor: 					{{base09-rgb-r}},{{base09-rgb-g}},{{base09-rgb-b}};

	--textbrightest: 				{{base07-rgb-r}},{{base07-rgb-g}},{{base07-rgb-b}};
	--textbrighter: 				{{base06-rgb-r}},{{base06-rgb-g}},{{base06-rgb-b}};
	--textbright: 					{{base05-rgb-r}},{{base05-rgb-g}},{{base05-rgb-b}};
	--textdark: 					  {{base04-rgb-r}},{{base04-rgb-g}},{{base04-rgb-b}};
	--textdarker: 			    calc( (2*{{base04-rgb-r}} + {{base03-rgb-r}}) / 3 ),calc( (2*{{base04-rgb-g}} + {{base03-rgb-g}}) / 3 ),calc( (2*{{base04-rgb-b}} + {{base03-rgb-b}}) / 3 );
	--textdarkest: 					{{base03-rgb-r}},{{base03-rgb-g}},{{base03-rgb-b}};

	--backgroundaccent: 			  {{base02-rgb-r}},{{base02-rgb-g}},{{base02-rgb-b}};
	--backgroundprimary: 			  calc( (2*{{base01-rgb-r}} + {{base02-rgb-r}}) / 3 ),calc( (2*{{base01-rgb-g}} + {{base02-rgb-g}}) / 3 ),calc( (2*{{base01-rgb-b}} + {{base02-rgb-b}}) / 3 );
	--backgroundsecondary: 	    calc( (5*{{base01-rgb-r}} + {{base00-rgb-r}}) / 6 ),calc( (5*{{base01-rgb-g}} + {{base00-rgb-g}}) / 6 ),calc( (5*{{base01-rgb-b}} + {{base00-rgb-b}}) / 6 );
	--backgroundsecondaryalt: 	calc( ({{base01-rgb-r}} + {{base00-rgb-r}}) / 2 ),calc( ({{base01-rgb-g}} + {{base00-rgb-g}}) / 2 ),calc( ({{base01-rgb-b}} + {{base00-rgb-b}}) / 2 );
	--backgroundtertiary: 	    calc( ({{base01-rgb-r}} + 3*{{base00-rgb-r}}) / 4 ),calc( ({{base01-rgb-g}} + 3*{{base00-rgb-g}}) / 4 ),calc( ({{base01-rgb-b}} + 3*{{base00-rgb-b}}) / 4 );
	--backgroundfloating: 			{{base00-rgb-r}},{{base00-rgb-g}},{{base00-rgb-b}};
